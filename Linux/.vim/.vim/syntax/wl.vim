"User Symbols
syntax match wlSymbol "[^0-9][A-Za-z0-9`$]\+\s*\%([@[]\|/:\|/\=/@\)\@=" contains=wlOperator,wlSystemSymbol,wlBrackets
syntax match wlSymbol "\<[^0-9][A-Za-z0-9`$]\+\>" contains=wlOperator,wlSystemSymbol,wlBrackets
syntax match wlSymbol "\~\s*[^~]\+\s*\~"ms=s+1,me=e-1 contains=wlOperator,wlSystemSymbol,wlBrackets
syntax match wlSymbol "//\s*[A-Za-z0-9`$]\+"ms=s+2 contains=wlOperator,wlSystemSymbol,wlBrackets

"Comments and TODOs
syntax keyword wlTodo TODO FIX FIXME BUG contained
syntax region wlComment start=+(\*+ end=+\*)+ skipempty contains=wlComment, wlTodo

"Strings
syntax region wlString start=+"+ skip=+\(\\\)\@<!\\"+ end=+"+ skipempty

"Numbers
syntax match wlNumber "\<\%(\d\+\.\=\d*\|\d*\.\=\d\+\)\>"
syntax match wlNumber "\d\+`\d*\%(\d\@!\.\|\>\)"
syntax match wlNumber "[A-Za-z]\+`\d\+\%(\d\@!\.\|\>\)" contains=wlSymbol,wlSystemSymbol

"Slots & Slotsequences
syntax match wlSlot "#\%(#\d*\|\d\+\)\="
syntax match wlSlot "#\%([A-Za-z0-9`$]*\)"

"Patterns
syntax match wlPattern "[A-Za-z0-9`$]*_\{1,3}\%([A-Za-z0-9`$]\+\)\="
syntax match wlPatternName "[A-Za-z0-9`$]\+\s*:[^:=>]"me=e-2
syntax match wlPattern "[A-Za-z0-9`]*_\+\%([A-Za-z0-9`]\+\)"

"Brackets
syntax match wlBrackets "[({[}\])]" contained
syntax match wlBrackets "\%(<|\||>\)" contained

"OPERATORS
"Apply, Postfix, MapAll, ReplaceRepeated
syntax match wlOperator "\%(@\{1,3}\|//[.@]\=\)"
"Condition, TagSet*, UpSet*, Map, ReplaceAll
syntax match wlOperator "\%(/[;:@.]\=\|\^\=:\==\)"
"Rule, RuleDelayed
syntax match wlOperator "\%([-:]\=>\|<=\=\)"
"SubtractFrom, AddTo, TimesBy, DivideBy
syntax match wlOperator "[-+*/]="
"Plus, Subtract, Times, Divide, Power, Optional/Default, PatternTest
syntax match wlOperator "[*+=^/.:?-]"
"Infix, StringExpression
syntax match wlOperator "\%(\~\~\=\)"
"Equal, SameQ, UnsameQ,
syntax match wlOperator "\%(=\{2,3}\|=\=!=\|||\=\|&&\|!\)"
"Composition, RightComposition
syntax match wlOperator "\%(@\*\|\/\*\)"

"Special characters
syntax match wlSpecial "\\\[[A-Za-z0-9]\+\]"

"Messages
syntax match wlMessage "::\s*[A-Za-z0-9]\+"hs=s+2

"Highlighting
if version >= 508 || !exists("did_wl_syn_inits")
	if version < 508
		let did_wl_syn_inits = 1
		command -nargs=+ HiLink hi link <args>
	else
		command -nargs=+ HiLink hi def link <args>
	endif

	if exists("g:wl_highlight_option")
		if g:wl_highlight_option == "default"
			"TODO implement default FE colors
		elseif g:wl_highlight_option == "solarized"
			HiLink wlComment      Comment
			HiLink wlSymbol       Function
			HiLink wlSystemSymbol Normal
			HiLink wlOperator     Normal
			HiLink wlSlot         Operator
			HiLink wlString       String
			HiLink wlSpecial      Function
			HiLink wlNumber       Normal
			HiLink wlPattern      Operator
			HiLink wlPatternName  Operator
			HiLink wlError        Error
			HiLink wlBracket      Normal
			HiLink wlTodo         Todo
			HiLink wlMessage      Type
		endif
	else
		HiLink wlComment      Comment
		HiLink wlSymbol       Normal
		HiLink wlSystemSymbol Function
		HiLink wlOperator     Operator
		HiLink wlSlot         Operator
		HiLink wlString       String
		HiLink wlSpecial      Normal
		HiLink wlNumber       Number
		HiLink wlPattern      Type
		HiLink wlPatternName  Type
		HiLink wlBracket      Normal
		HiLink wlTodo         Todo
		HiLink wlMessage      Type
	endif

	delcommand HiLink
endif
